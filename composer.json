{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "7.0.*",
        "ext-gd": "*",
        "barryvdh/laravel-snappy": "^0.4.1",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~3.3",
        "h4cc/wkhtmltopdf-amd64": "*",
        "intervention/image": "dev-master",
        "jenssegers/rollbar": "^1.5",
        "laravel/framework": "5.5.*",
        "laravel/socialite": "^2.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "maatwebsite/excel": "~2.1.0",
        "predis/predis": "^1.1",
        "segmentio/analytics-php": "^1.5",
        "spatie/laravel-newsletter": "^4.2",
        "stripe/stripe-php": "3.*",
        "webpatser/laravel-uuid": "^3.0",
        "wildbit/laravel-postmark-provider": "^3.0",
        "wildbit/postmark-php": "^2.3"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "jakub-onderka/php-console-highlighter": "^0.3.2",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "mockery/mockery": "~1.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "~6.0",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^4.1",
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }, "files": ["app/Http/helpers.php","config/constants.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "mkdir -p storage/framework/views;mkdir -p storage/framework/sessions;mkdir -p storage/framework/cache;chmod 777 -R storage",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "mkdir -p storage/framework/views;mkdir -p storage/framework/sessions;mkdir -p storage/framework/cache;chmod 777 -R storage",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
